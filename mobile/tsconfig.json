{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@Components/*": [
        "src/components/*"
      ],
      "@Redux/*": [
        "src/redux/*"
      ],
      "@Routes/*": [
        "src/routes/*"
      ],
      "@Functions": [
        "src/functions/index"
      ],
    }
  },
  "typeRoots": [
    "./types"
  ],
}